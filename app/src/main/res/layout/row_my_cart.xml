<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="myCartRowModel" type="com.codewaresapplication.app.modules.mycart.data.model.MyCartRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowunsplash8hqpxt"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageUnsplash8HqPXT"
            android:layout_height="@dimen/_171pxh"
            android:layout_width="@dimen/_142pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_20pxh}"
            imageRes="@{@drawable/img_unsplash8hqpxt}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_unsplash8hqpxt"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumncasual"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtCasual"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{myCartRowModel.txtCasual}"
                style="@style/txtPoppinsmedium18" tools:text="@string/lbl_casual"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCasualJeansSh"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{myCartRowModel.txtCasualJeansSh}"
                style="@style/txtPoppinsregular15" tools:text="@string/msg_casual_jeans_sh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSizeXL"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{myCartRowModel.txtSizeXL}"
                style="@style/txtPoppinsregular14_1" tools:text="@string/lbl_size_xl"/>
            <TextView android:ellipsize="end" android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{myCartRowModel.txtPrice}"
                style="@style/txtPoppinssemibold18" tools:text="@string/lbl_134_98"/>
            <LinearLayout android:id="@+id/linearRowone"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackone"
                    android:layout_height="@dimen/_28pxv" android:layout_width="@dimen/_83pxh">
                    <LinearLayout android:id="@+id/linearColumnone"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylegray_102">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtOne"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{myCartRowModel.txtOne}"
                            style="@style/txtPoppinsmedium15" tools:text="@string/lbl_1"/>
                    </LinearLayout>
                    <FrameLayout
                        android:id="@+id/frameStackellipseeleven"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_28pxv" android:layout_width="@dimen/_28pxh">
                        <LinearLayout
                            android:id="@+id/linearColumnellipseeleven"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                            <View
                                android:background="@drawable/rectangle_bg_gray_102_radius_11_9"
                                android:id="@+id/viewEllipseEleven"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_23pxh" android:layout_width="@dimen/_23pxh"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTwo"
                            android:layout_gravity="bottom|center"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{myCartRowModel.txtTwo}"
                            style="@style/txtPoppinssemibold15" tools:text="@string/lbl"/>
                    </FrameLayout>
                    <FrameLayout
                        android:id="@+id/frameStackellipseelevenOne"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_28pxv" android:layout_width="@dimen/_28pxh">
                        <LinearLayout
                            android:id="@+id/linearColumnellipseelevenOne"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                            <View
                                android:background="@drawable/rectangle_bg_gray_102_radius_11_9"
                                android:id="@+id/viewEllipseElevenOne"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_23pxh" android:layout_width="@dimen/_23pxh"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtThree"
                            android:layout_gravity="bottom|end"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{myCartRowModel.txtThree}"
                            style="@style/txtPoppinssemibold15" tools:text="@string/lbl4"/>
                    </FrameLayout>
                </FrameLayout>
                <ImageView android:id="@+id/imageClose"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
