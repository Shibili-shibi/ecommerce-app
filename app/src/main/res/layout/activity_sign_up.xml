<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpVM" type="com.codewaresapplication.app.modules.signup.data.viewmodel.SignUpVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSignUp"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarAppbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLetsSignUp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtLetsSignUp}"
                    style="@style/txtPoppinsmedium18" tools:text="@string/lbl_let_s_sign_up"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <TextView android:id="@+id/txtCreateAccount"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_9pxh"
            android:text="@{signUpVM.signUpModel.txtCreateAccount}"
            style="@style/txtPoppinsmedium30" tools:text="@string/lbl_create_account"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_201_radius_10"
            android:ellipsize="end" android:fontFamily="@font/poppins"
            android:hint="@string/lbl_name" android:id="@+id/etName"
            android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:paddingBottom="@dimen/_18pxv"
            android:paddingEnd="@dimen/_281pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_18pxv" android:singleLine="true"
            android:text="@={signUpVM.signUpModel.etNameValue}"
            android:textColor="@color/black_900"
            android:textColorHint="@color/black_900"
            android:textFontWeight="400"
            android:textSize="@dimen/_15pxh" android:textStyle="normal" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_201_radius_10"
            android:ellipsize="end" android:fontFamily="@font/poppins"
            android:hint="@string/lbl_email" android:id="@+id/etNameOne"
            android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:paddingBottom="@dimen/_19pxv"
            android:paddingEnd="@dimen/_286pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_17pxv" android:singleLine="true"
            android:text="@={signUpVM.signUpModel.etNameOneValue}"
            android:textColor="@color/black_900"
            android:textColorHint="@color/black_900"
            android:textFontWeight="400"
            android:textSize="@dimen/_15pxh" android:textStyle="normal" tools:ignore="Autofill"/>
        <EditText
            android:background="@drawable/rectangle_bg_gray_201_radius_10"
            android:drawableEnd="@drawable/img_location"
            android:drawablePadding="@dimen/_30pxh"
            android:ellipsize="end" android:fontFamily="@font/poppins"
            android:hint="@string/lbl_password"
            android:id="@+id/etGroup964" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_1pxh"
            android:paddingBottom="@dimen/_18pxv"
            android:paddingEnd="@dimen/_16pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_18pxv" android:singleLine="true"
            android:text="@={signUpVM.signUpModel.etGroup964Value}"
            android:textColor="@color/black_900"
            android:textColorHint="@color/black_900"
            android:textFontWeight="400"
            android:textSize="@dimen/_15pxh" android:textStyle="normal" tools:ignore="Autofill"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnsignUp"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius">
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_black_900_radius_10"
                android:ellipsize="end"
                android:fontFamily="@font/poppins"
                android:gravity="center" android:id="@+id/btnSignUp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_141pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_sign_up"
                android:textAllCaps="false"
                android:textColor="@color/white_A700"
                android:textFontWeight="500"
                android:textSize="@dimen/_16pxh"
                android:textStyle="bold" tools:text="@string/lbl_sign_up"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtConfirmation"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtConfirmation}"
                style="@style/txtPoppinsmedium13" tools:text="@string/msg_already_have_an"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowrectangletwenty"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius_1">
            <View android:background="@drawable/rectangle_bg_black_900"
                android:id="@+id/lineRectangleTwenty"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_106pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrcontinuewit"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtOrcontinuewit}"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_or_continue_wit"/>
            <View android:background="@drawable/rectangle_bg_black_900"
                android:id="@+id/lineRectangleTwentyOne"
                android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_106pxh"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowgoogle"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_gray_102_radius_7"
                android:drawablePadding="@dimen/_16pxh"
                android:drawableStart="@drawable/img_google"
                android:ellipsize="end"
                android:fontFamily="@font/poppins"
                android:gravity="center" android:id="@+id/btnGoogle"
                android:layout_height="@dimen/_52pxv"
                android:layout_width="@dimen/_164pxh"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_32pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_google"
                android:textAllCaps="false"
                android:textColor="@color/black_900"
                android:textFontWeight="400"
                android:textSize="@dimen/_15pxh"
                android:textStyle="normal" tools:text="@string/lbl_google"/>
            <androidx.appcompat.widget.AppCompatButton
                android:background="@drawable/rectangle_bg_gray_102_radius_7"
                android:drawablePadding="@dimen/_16pxh"
                android:drawableStart="@drawable/img_group_917"
                android:ellipsize="end"
                android:fontFamily="@font/poppins"
                android:gravity="center" android:id="@+id/btnApple"
                android:layout_height="@dimen/_52pxv"
                android:layout_width="@dimen/_163pxh"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_36pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_apple"
                android:textAllCaps="false"
                android:textColor="@color/black_900"
                android:textFontWeight="400"
                android:textSize="@dimen/_15pxh"
                android:textStyle="normal" tools:text="@string/lbl_apple"/>
        </LinearLayout>
    </LinearLayout>
</layout>
