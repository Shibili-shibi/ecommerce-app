<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="orderStatusRowModel" type="com.codewaresapplication.app.modules.orderstatus.data.model.OrderStatusRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowgroup541"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageGroup541"
            android:layout_height="@dimen/_67pxh"
            android:layout_width="@dimen/_7pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_group541"
            tools:ignore="ContentDescription" tools:src="@drawable/img_group541"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearOrderReceived"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrderReceived"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{orderStatusRowModel.txtOrderReceived}"
                style="@style/txtPoppinsregular18_1" tools:text="@string/lbl_order_received"/>
            <LinearLayout android:id="@+id/linearRowclock"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageClockOne"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_clock_16X16"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_clock_16X16"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTimeOne"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{orderStatusRowModel.txtTimeOne}"
                    style="@style/txtPoppinsmedium12" tools:text="@string/msg_05_48_pm_7_jun"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
