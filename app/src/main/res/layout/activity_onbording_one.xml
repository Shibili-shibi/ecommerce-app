<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="onbordingOneVM" type="com.codewaresapplication.app.modules.onbordingone.data.viewmodel.OnbordingOneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameOnbordingOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageUnsplashKj6LgG"
            android:layout_gravity="start"
            android:layout_height="@dimen/_767pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_unsplashkj6lgg}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_unsplashkj6lgg"/>
        <FrameLayout android:id="@+id/frameStackshade"
            android:layout_gravity="start"
            android:layout_height="match_parent" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageShade"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_shade_1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_shade_1"/>
            <LinearLayout android:id="@+id/linearColumnarrowleft"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarAppbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_width="@dimen/_40pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGetStarted"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{onbordingOneVM.onbordingOneModel.txtGetStarted}"
                            style="@style/txtPoppinsmedium18_2" tools:text="@string/lbl_get_started"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAllForYou"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{onbordingOneVM.onbordingOneModel.txtAllForYou}"
                    style="@style/txtPoppinsmedium24" tools:text="@string/lbl_all_for_you"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_12pxh"
                    android:text="@{onbordingOneVM.onbordingOneModel.txtDescription}"
                    style="@style/txtPoppinsregular14_2" tools:text="@string/msg_we_always_try_p"/>
                <FrameLayout android:gravity="center_horizontal"
                    android:id="@+id/frameGroup1304"
                    android:layout_height="wrap_content" android:layout_width="match_parent"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
